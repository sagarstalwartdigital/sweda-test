        $('.placeholder').mask("00/00/0000", {placeholder: "__/__/____"});
        $('.fallback').mask("00r00r0000", {
          translation: {
            'r': {
              pattern: /[\/]/,
              fallback: '/'
            },
            placeholder: "__/__/____"
          }
        });
        $('.selectonfocus').mask("00/00/0000", {selectOnFocus: true});
        var SPMaskBehavior = function (val) {
          return val.replace(/\D/g, '').length === 11 ? '(00) 00000-0000' : '(00) 0000-00009';
        },
        spOptions = {
          onKeyPress: function(val, e, field, options) {
              field.mask(SPMaskBehavior.apply({}, arguments), options);
            }
        };
        $('.sp_celphones').mask(SPMaskBehavior,{placeholder: "(00) 00000-0000"});

        var FaxMaskBehavior = function (val) {
          return val.replace(/\D/g, '').length === 11 ? '0-000-0000000' : '0-000-0000000';
        },
        spOptions = {
          onKeyPress: function(val, e, field, options) {
              field.mask(FaxMaskBehavior.apply({}, arguments), options);
            }
        };
        $(".fax").mask(FaxMaskBehavior,{placeholder: "_-___-_______"});
    });


