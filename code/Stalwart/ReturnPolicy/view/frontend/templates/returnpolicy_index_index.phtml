    $(document).ready(function(){


        //// Add Multipal Field    
        var max_fields = 50; 
        var wrapper    = $(".wrapper");
        var add_button = $(".add_fields");
        var x = 1;
        $(add_button).click(function(e){
            e.preventDefault();
